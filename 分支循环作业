#include <stdio.h>
#include <string.h>
int main()
{
  char password[20] = {0};
  printf("请输入密码:>");
  scanf("%s", password);
  for (int i = 0; i < 3; i++)
  {
    if (strcmp(password, "123456") == 0)
    {
      printf("登录成功");
      break;
    }
    else
      printf("登陆失败，请重新登录");
    if (i == 3)
      break;
  }

  return 0;
}
