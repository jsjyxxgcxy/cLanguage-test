#include <stdio.h>
int main()
{

  int m = 0;
  int n = 0;
  int r = 0;
  printf("请从大到小输入两个数:>");
  scanf("%d%d", &m, &n);
  while (m % n)
  {
    r = m % n;
    m = n;
    n = r;
  }
  printf("%d\n", n);
  return 0;
}
