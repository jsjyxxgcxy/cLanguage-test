#include <stdio.h>
int main()
{
  char arr[5] = "*****";
  int i = 0;
  for (i = 0; i < 5; i++)
  {
    int j = 0;
    for (j = 0; j < i; j++)
      printf("  ");
    for (j = 0; j < 5; j++)
      printf("%c ", arr[j]);
    printf("\n");
  }
  return 0;
}
