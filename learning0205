#include <stdio.h>
int jiechen(int num)
{
  int i = 0;
  int b = 1;
  for (i = 1; i <= num; i++)
  {
    b *= i;
  }
  return b;
}
int main()
{
  int i;
  int sum = 0;
  for (i = 1; i < 11; i++)
  {
    sum += jiechen(i);
  }
  printf("%d\n", sum);
  printf("%d", jiechen(5));
  return 0;
}
